<Window x:Class="WebBrowser.Window3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WebBrowser"
        xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
        mc:Ignorable="d"
        Title="Nastavení" Height="263" Width="800">
    <Grid>
        <Label HorizontalAlignment="Left" Height="20" Margin="10,10,0,0" VerticalAlignment="Top" Width="17" Content="{ui:SymbolIcon Settings48, Filled=True}"/>
        <Label Content="Nastavení" FontSize="24" HorizontalAlignment="Left" Margin="32,8,0,0" VerticalAlignment="Top" Height="22" Width="79" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}" FontWeight="Bold"/>
        <TextBox x:Name="urlb" HorizontalAlignment="Left" Margin="166,28,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="624" MinWidth="6"/>
        <Label Content="Homepage - URL adresa" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" Foreground="{DynamicResource {x:Static SystemColors.ActiveCaptionTextBrushKey}}"/>
        <Button Content="Uložit" Margin="10,10,10,10" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Click="Button_Click"/>

    </Grid>
</Window>
